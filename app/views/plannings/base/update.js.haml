App.plannings.selectable.clear();
App.plannings.selectable.destroy();
$('#weeks_header_#{dom_id(@board.subject)}').html('#{j(render 'calendar_weeks')}');
$('#planned_#{dom_id(@board.subject)}').html('#{j(render('header_planned'))}');
